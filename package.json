{
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV=production webpack --mode production",
    "watch": "webpack serve --hot --mode development",
    "build:dev": "webpack --mode development",
    "build:size": "NODE_ENV=production webpack --json | webpack-bundle-size-analyzer",
    "build:ci": "yarn lint && yarn test:cov && yarn build",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --collectCoverage",
    "lint": "eslint src",
    "deploy": "yarn build && netlify deploy"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-flow": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.124.0",
    "jest": "^26.0.1",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^2.1.0",
    "prettier": "^2.0.5",
    "prettier-eslint": "^9.0.1",
    "prettier-eslint-cli": "^5.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^1.2.1",
    "webpack": "^5.40.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.10.3"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
