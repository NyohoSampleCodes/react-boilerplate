{
  "scripts": {
    "start": "webpack-cli serve --mode development",
    "build": "NODE_ENV=production webpack --mode production",
    "watch": "webpack serve --hot --mode development",
    "build:dev": "webpack --mode development",
    "build:size": "NODE_ENV=production webpack --json | webpack-bundle-size-analyzer",
    "build:ci": "yarn lint && yarn test:cov && yarn build",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --collectCoverage",
    "lint": "eslint src",
    "deploy": "yarn build && netlify deploy"
  },
  "dependencies": {
    "babel-plugin-transform-runtime": "^6.23.0",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-flow": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "babel-plugin-module-resolver": "^5.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "flow": "^0.2.3",
    "flow-bin": "^0.197.0",
    "jest": "^29.3.1",
    "jest-css-modules": "^2.1.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.3",
    "prettier-eslint": "^15.0.1",
    "prettier-eslint-cli": "^7.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>/node_modules/jest-css-modules"
    }
  }
}
